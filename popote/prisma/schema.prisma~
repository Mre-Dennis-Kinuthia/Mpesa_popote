// prisma/schema.prisma

// Define your database connection
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Define your data models
model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}